package com.ztesoft.net.outter.inf.huasheng;

import java.io.Serializable;

public class HuaShengGoodsItemsDetail  implements Serializable{

	private String MATNR = "";	//物料号
	private String MAKTX = "";	//物料描述
	private String MEINS = "";	//基本单位
	private String MTART = "";	//物料类型
	private String MATKL = "";	//物料组
	private String XCHPF = "";	//批次管理
	private String MCHK = "";	//串码管理
	private String CMMAG = "";	//串码长度
	private String RESERVE2 = "";	//预留字段2
	private String RESERVE3 = "";	//预留字段3
	
	public String getMATNR() {
		return MATNR;
	}
	public void setMATNR(String mATNR) {
		MATNR = mATNR;
	}
	public String getMAKTX() {
		return MAKTX;
	}
	public void setMAKTX(String mAKTX) {
		MAKTX = mAKTX;
	}
	public String getMEINS() {
		return MEINS;
	}
	public void setMEINS(String mEINS) {
		MEINS = mEINS;
	}
	public String getMTART() {
		return MTART;
	}
	public void setMTART(String mTART) {
		MTART = mTART;
	}
	public String getMATKL() {
		return MATKL;
	}
	public void setMATKL(String mATKL) {
		MATKL = mATKL;
	}
	public String getXCHPF() {
		return XCHPF;
	}
	public void setXCHPF(String xCHPF) {
		XCHPF = xCHPF;
	}
	public String getMCHK() {
		return MCHK;
	}
	public void setMCHK(String mCHK) {
		MCHK = mCHK;
	}
	public String getCMMAG() {
		return CMMAG;
	}
	public void setCMMAG(String cMMAG) {
		CMMAG = cMMAG;
	}
	public String getRESERVE2() {
		return RESERVE2;
	}
	public void setRESERVE2(String rESERVE2) {
		RESERVE2 = rESERVE2;
	}
	public String getRESERVE3() {
		return RESERVE3;
	}
	public void setRESERVE3(String rESERVE3) {
		RESERVE3 = rESERVE3;
	}
	
	
	
}
