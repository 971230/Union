@charset "utf-8";
/* CSS Document */
body{ background:#242424; color:#fff;}
a{ color:#fff; cursor:pointer;}
a:hover{ color:#FF8714;}
/*头部样式*/
.topbar{ background:url(/public/common/ress/images/black/topbar.jpg) left top repeat-x; height:45px; line-height:40px; color:#fff; font-size:16px; padding-left:10px; position:relative;}
.loginDiv{ width:160px; text-align:center; position:absolute; right:0px; top:0px; height:41px; line-height:41px;}
.loginBtn{ background:#ff7200 url(/public/common/ress/images/black/btn.png) repeat-x; display:inline-block;height:27px; padding-left:13px; padding-right:13px;line-height:27px;color:#fff; text-align:center; border:1px solid #ffbb7f;border-radius: 3px;-moz-border-radius:3px; -webkit-border-radius:3px;box-shadow:0 0 10px 0 rgba(0,0,0,0.4); font-size:16px; cursor:pointer;}
.loginBtn:hover, .loginBtn.curr{ background:#ff6000 url(/public/common/ress/images/black/btnhover.png) repeat-x; border:1px solid #ffc287;-webkit-box-shadow:1px 1px 5px #ffa22e;-moz-box-shadow:1px 1px 5px #ffa22e; box-shadow:1px 1px 5px #ffa22e; color:#fff;}
.grayBtn{ background:#0690d2 url(/public/common/ress/images/black/graybtn.png) repeat-x; display:inline-block;height:27px; padding-left:13px; padding-right:13px;line-height:27px;color:#fff; text-align:center; border:1px solid #b3b3b3;border-radius: 3px;-moz-border-radius:3px; -webkit-border-radius:3px;box-shadow:0 0 10px 0 rgba(0,0,0,0.4); font-size:16px; cursor:pointer;}
.grayBtn:hover, .grayBtn.curr{ background:#0690d2 url(/public/common/ress/images/black/graybtn.png) repeat-x; border:1px solid #b3b3b3;-webkit-box-shadow:1px 1px 4px #d7d7d7;-moz-box-shadow:1px 1px 4px #d7d7d7; box-shadow:1px 1px 4px #d7d7d7;color:#fff;}


/*头部样式结束*/
/*底部样式*/
.footer{ background:url(/public/common/ress/images/black/footerbar.jpg) left top repeat-x;width:100%; height:47px;color:#fff; font-size:14px;}
.footerList li.first{border-left:none;}
.footerList li.last{border-right:none;}
.footerList{ padding:0px; margin:0px; display:block;}
.footerList li{ width:25%; float:left; text-align:center;}
.footerList li a{ display:inline-block; width:100%;border-right:1px solid #424444; height:45px; border-left:1px solid #2c2c2c; padding-top:2px;}
.footerList li a:hover,.footerList li a.curr{ background:url(/public/common/ress/images/black/footerhover.jpg) repeat-x; color:#fff; height:45px;}
.footerIcon{ display:inline-block; width:22px; height:22px;}
.footerhome{ background:url(/public/common/ress/images/black/footerhome.png) no-repeat;}
.footerhelp{ background:url(/public/common/ress/images/black/footerhelp.png) no-repeat;}
.footersearch{ background:url(/public/common/ress/images/black/footersearch.png) no-repeat;}
.footertop{ background:url(/public/common/ress/images/black/footertop.png) no-repeat;}
/*底部样式结束*/

/*页面body*/
.wapper{}
/*页面body结束*/

/*banner*/
.bannerDiv{ text-align:center; height:100px; background:#242424; padding-left:10px;padding-right:10px;}
.bannerImg{ width:100%; height:100px;}

/*栏目*/
.columnDiv{margin-top:5px;}

/*栏目标题*/
.columnTitle{ background:url(/public/common/ress/images/black/blacktitle.jpg) repeat-x; height:45px; line-height:40px; color:#fff;padding-left:10px; position:relative;}
.columnTitle h2{ color:#fff; font-size:16px; font-weight:lighter;}
.columnContent{ padding:10px;}

.phonePlatContent{ padding-top:10px; padding-bottom:10px;}
.channelList{ padding:0px; margin:0px;}
.channelList li{ width:20%; float:left; text-align:center; margin-bottom:10px;}
.channelList li a{ background:#3e3e3e url(/public/common/ress/images/black/blackiconbg.jpg) left top repeat-x; display:inline-block;  width:92%; text-align:center;border-radius: 5px;-moz-border-radius:5px; -webkit-border-radius:5px;box-shadow:0 0 10px 0 rgba(0,0,0,0.4); padding-top:5px; color:#b0afaf; font-size:18px; border-left:1px solid #464646;border-right:1px solid #464646; font-size:16px; padding-bottom:5px;}
.channelList li a:hover,.channelList li a.curr{ background:#fd7d02 url(/public/common/ress/images/black/blackiconbg_hover.jpg) left top repeat-x; border-left:1px solid #ffb368;border-right:1px solid #ffb368;box-shadow:none; color:#fff; }
.allicon{ background:url(/public/common/ress/images/black/allicon.png) left top no-repeat; display:inline-block; width:50px; height:45px;}
.liveicon{ background-position:0px 0px;}
.newsicon{background-position:-50px 0px;}
.filmtvicon{background-position:-100px 0px;}
.yuleicon{background-position:-150px 0px;}
.musicicon{background-position:-200px 0px;}
.fashionicon{background-position:0px -50px;}
.originalicon{background-position:-50px -50px;}
.healthicon{background-position:-100px -50px;}
.columnicon{background-position:-150px -50px;}
.phoneicon{background-position:-200px -50px;}
.channelList li a:hover .liveicon,.channelList li a.curr .liveicon{background-position:0px -100px;}
.channelList li a:hover .newsicon,.channelList li a.curr .newsicon{background-position:-50px -100px;}
.channelList li a:hover .filmtvicon,.channelList li a.curr .filmtvicon{background-position:-100px -100px;}
.channelList li a:hover .yuleicon,.channelList li a.curr .yuleicon{background-position:-150px -100px;}
.channelList li a:hover .musicicon,.channelList li a.curr .musicicon{background-position:-200px -100px;}
.channelList li a:hover .fashionicon,.channelList li a.curr .fashionicon{background-position:0px -150px;}
.channelList li a:hover .originalicon,.channelList li a.curr .originalicon{background-position:-50px -150px;}
.channelList li a:hover .healthicon,.channelList li a.curr .healthicon{background-position:-100px -150px;}
.channelList li a:hover .columnicon,.channelList li a.curr .columnicon{background-position:-150px -150px;}
.channelList li a:hover .phoneicon,.channelList li a.curr .phoneicon{background-position:-200px -150px;}

/*轮播图*/
.flashDiv{ text-align:center;}
.flashimg{ width:100%; margin:0 auto;}
.flashimgWidth{ width:100%; height:140px; }
.flashOn{ text-align:center;height:24px;width:100%; margin:0 auto; background:#A6A6A6; position:relative; padding:0px;}
.slide-dot {bottom: 5px; height: 9px; padding: 2px 0; position: absolute; right: 8px;z-index:9999;}
.dotIcon{ background:url(/public/common/ress/images/black/slide-dot.png) center center no-repeat; width:8px; height:9px; display:inline-block; vertical-align:top;}
.dotListicon{ background-position:0px 0px; margin-right:5px; margin-left:5px;}
.dotListicon:hover,.dotListicon.curr{ background-position:-8px 0px;}

.titleListDiv{ padding-left:105px; height:40px; line-height:40px; position:relative;}
.iptDiv{ position:absolute; left:0px; top:0px; height:40px; width:100px;}
.searchDiv{ position:absolute; right:10px; top:0px; height:40px; line-height:40px; text-align:right; width:80px; font-size:14px;}
.iptbg{ background:#fff url(/public/common/ress/images/black/iptbg.jpg) repeat-x; height:32px; line-height:32px; border:none; width:100%; color:#979191; font-size:16px;font-family:'Microsoft Yahei',\5FAE\8F6F\96C5\9ED1,sans-serif;border-radius: 5px;-moz-border-radius:5px; -webkit-border-radius:5px;}
.titleList{ padding:0px; margin:0px; display:block;}
.titleList li{ float:left; padding-left:5px; padding-right:5px; position:relative; font-size:12px;}
.titleList li a{ color:#fff; text-decoration:none;}
.titleList li a:hover{color:#FF8714;text-decoration:underline;}

.movieTitleList{ padding:0px; margin:0px;}
.movieTitleList li{ height:41px; line-height:40px; float:left;border-right:1px solid #2a2a2a; border-left:1px solid #494949;}
.movieTitleList li a{ padding-left:10px; padding-right:10px; display:inline-block; font-size:16px;height:41px;}
.movieTitleList li a:hover,.movieTitleList li a.curr{ background:#1c1d1f; color:#ed6d00;-moz-box-shadow:3px 3px 3px #161616 inset;-webkit-box-shadow:3px 3px 3px #161616 inset; box-shadow:3px 3px 3px #161616 inset;}
.movieTitleList li.first{ border-left:none;}
.movieTitleList li.last{ border-right:none;}

.alineDiv{ padding-left:25px; padding-right:25px; position:relative;}
