/*轮播图*/
#demo1-wrapper {	
		width: 300px;
		height: 150px;
		overflow: hidden;
		background: #DDDDDD;
		margin:0 auto;	
	}
	
	#demo1 {
		height: 150px;
		width: 1500px;
		-webkit-perspective: 1000;
	}
		
	.card, .front, .back {
		border-radius: 10px;
		color: #fff;
		float: left;
		height: 150px;
		line-height: 150px;
		position: relative;
		padding: 0;
		text-align: center;
		width: 300px;
	}
	
	.front, .back {
		position: absolute;
		top: 0;
		left: 0;
		margin: 0;
		-webkit-backface-visibility: hidden;
	}
	
	.back {
		color: #fff;
		-webkit-transform: rotateY(180deg) translate3d(0,0,0);
		background: url(img/fabric.png);
	}
	
	.card {
		-webkit-transform-style: preserve-3d;
		-webkit-animation-name: default;
		-webkit-animation-duration: 0.5s;
		-webkit-animation-timing-function: ease-in-out;
		-webkit-animation-iteration-count: 1;
		-webkit-animation-fill-mode: forwards;
	}
	
	.card.flipped {
		-webkit-animation-name: cardflipped;
	}
	.demo1-wrapper{ position:relative;}
	.flashtrans{ position:absolute; bottom:0px; left:0px; right:0px; height:25px; line-height:25px; background:#000;filter:alpha(opacity=60); -moz-opacity:0.6; -khtml-opacity: 0.6; opacity: 0.6; z-index:9997;}
	.flashtransTitle{ color:#fff; font-size:16px; text-align:center; position:absolute;bottom:0px; left:0px; right:0px; height:25px; line-height:25px;z-index:9998;font-family:'Microsoft Yahei',\5FAE\8F6F\96C5\9ED1,sans-serif;}

	#card1 .front { color: #000; }	
	#card2 .front { background: #fff; }
	#card3 .front { background: #fff; }
	#card4 .front { background: #fff; }
	#card5 .front { background: #fff; }
	
	@-webkit-keyframes default { 
		from 	{ -webkit-transform: rotateY(180deg) translate3d(0,0,0); }
		to 		{ -webkit-transform: rotateY(0deg) translate3d(0,0,0); }
	}

	@-webkit-keyframes cardflipped { 
		from 	{ -webkit-transform: rotateY(0deg) translate3d(0,0,0); }
		to 		{ -webkit-transform: rotateY(180deg) translate3d(0,0,0); }
	}


