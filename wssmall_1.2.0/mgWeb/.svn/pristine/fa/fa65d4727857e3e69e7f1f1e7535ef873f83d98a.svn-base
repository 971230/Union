body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
form,
fieldset,
button,
input,
textarea,
hr,
p,
blockquote,
th,
td {
  margin: 0;
  padding: 0;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
  width: 100%;
}
fieldset,
img {
  border: 0;
}
address,
caption,
cite,
code,
dfn,
em,
i,
strong,
th,
var {
  font-style: normal;
  font-weight: normal;
}
button {
  background: none;
  border: none;
  cursor: pointer;
  display: inline-block;
  font-size: 14px;
  vertical-align: middle;
}
ol,
ul,
li {
  list-style: none;
}
hr {
  height: 0;
  border: none;
}
caption,
th {
  text-align: left;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 100%;
  font-weight: normal;
}
a,
a:hover,
a:active,
a:link {
  text-decoration: none;
}
/*CSS reset*/
html,
body {
  height: 100%;
}
body {
  font-size: 12px;
  font-weight: normal;
  background: #fbfbfb;
  color: #5d5f63;
}
body,
button,
input,
textarea {
  font-family: "microsoft yahei", "Droid Sans", "Helvetica Neue", "Droid Sans Fallback", "Heiti SC", "Hiragino Sans GB", Simsun, Tahoma, Arial, Roboto, sans-self;
  font-size: 12px;
  outline: none;
}
a,
a:hover,
a:active,
a:link {
  color: #5d5f63;
}
strong {
  font-weight: bold;
}
th,
td {
  padding: 5px 10px;
}
.clear-fix:after {
  display: block;
  clear: both;
  content: "";
  visibility: hidden;
  height: 0;
}
.clear-fix {
  zoom: 1;
}
.clear {
  clear: both;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.d-block {
  display: block;
}
.d-inline {
  display: inline;
}
.d-inline-block {
  display: inline-block;
}
.ie-inline-block {
  *zoom: 1;
  *display: inline;
}
.text-center {
  text-align: center;
}
.text-left {
  text-align: left;
}
.text-right {
  text-align: right;
}
.vertical-middle {
  vertical-align: middle;
}
.vertical-top {
  vertical-align: top;
}
.vertical-bottom {
  vertical-align: bottom;
}
.font12 {
  font-size: 12px;
}
.font14 {
  font-size: 14px;
}
.font16 {
  font-size: 16px;
}
.font18 {
  font-size: 18px;
}
.font24 {
  font-size: 24px;
}
.font26 {
  font-size: 26px;
}
.ellipsis {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.red {
  color: #dd0000;
}
.blue {
  color: #4d99d5;
}
.gery {
  color: #4c4c4c;
}
.org {
  color: #ff410e;
}
.greed {
  color: #7cb342;
}
.pink {
  color: #e40077;
}
.mt5 {
  margin-top: 5px;
}
.mt10 {
  margin-top: 10px;
}
.mt20 {
  margin-top: 20px;
}
.mb10 {
  margin-bottom: 10px;
}
.mb20 {
  margin-bottom: 20px;
}
.ml5 {
  margin-left: 5px;
}
.ml10 {
  margin-left: 10px;
}
.ml20 {
  margin-left: 20px;
}
.mr5 {
  margin-right: 5px;
}
.mr10 {
  margin-right: 10px;
}
.mr20 {
  margin-right: 10px;
}
.mlr5 {
  margin-left: 5px;
  margin-right: 5px;
}
.mlr10 {
  margin-left: 10px;
  margin-right: 10px;
}
.mlr20 {
  margin-left: 20px;
  margin-right: 20px;
}
.mtb10 {
  margin-top: 10px;
  margin-bottom: 10px;
}
.mtb20 {
  margin-top: 20px;
  margin-bottom: 20px;
}
.btn-middle {
  margin-top: -5px;
  vertical-align: middle;
}
.p10 {
  padding: 10px;
}
.p20 {
  padding: 20px;
}
.dashed-border {
  border-bottom: 1px dashed #e5e5e5;
  height: 0;
  display: block;
}
.wrap {
  position: relative;
  min-height: 100%;
}
.main {
  margin: 0 auto;
  width: 960px;
  padding-bottom: 160px;
}
/*外层布局包围*/
.page {
  padding: 20px;
  text-align: center;
}
.page .page-link {
  height: 30px;
  line-height: 30px;
  padding: 0 15px;
  font-size: 14px;
  text-align: center;
  margin: 0 5px;
  background: #fff;
  border: 1px solid #e5e5e5;
  display: inline-block;
  *zoom: 1;
  *display: inline;
}
.page .page-link:hover,
.page .page-link.active {
  background: #f58756;
  border-color: #f58756;
  color: #fff;
}
.head-wrap,
.head-main {
  margin: 0 auto;
  background: #fff;
}
.head-wrap {
  min-width: 960px;
  height: 80px;
}
.head-main {
  margin: 0 auto;
  width: 960px;
  padding-top: 16px;
}
.head-main .logo,
.head-main .project-name {
  float: left;
}
.head-main .logo,
.head-main .logo img {
  width: 130px;
  height: 44px;
}
.head-main .project-name {
  padding: 14px 0 0 20px;
  font-size: 18px;
  color: #000;
}
.head-main .hd-search-box {
  position: relative;
  width: 277px;
  height: 32px;
  border: 1px solid #ff9900;
  padding: 0 54px 0 10px;
  margin-top: 5px;
  border-radius: 2px;
}
.head-main .hd-search-box .hd-search-btn {
  background: #ff9900;
  color: #fff;
  width: 42px;
  height: 32px;
  font-size: 16px;
  position: absolute;
  right: 0;
  top: 0;
}
.head-main .hd-search-box .hd-search-text {
  width: 100%;
  height: 32px;
  line-height: 32px;
  color: #5d5f63;
  font-size: 14px;
  border: none;
  background: none;
}
.nav-wrap,
.nav-main {
  background: #f36f2a;
  height: 44px;
}
.nav-wrap {
  min-width: 960px;
}
.nav-main {
  margin: 0 auto;
  width: 960px;
}
.nav-main .nav-list .nav-item {
  float: left;
  font-size: 14px;
  text-align: center;
}
.nav-main .nav-list .nav-link {
  display: block;
  width: 120px;
  padding: 0 20px;
  height: 44px;
  line-height: 44px;
  color: #fff;
  background: #f36f2a;
}
.nav-main .nav-list .nav-link:hover,
.nav-main .nav-list .nav-link.active {
  background: #ed4c18;
}
.foot-wrap {
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  min-width: 960px;
  border-top: 2px solid #f36f2a;
}
.foot-wrap,
.foot-content {
  background: #fff;
}
.foot-content {
  margin: 0 auto;
  padding: 20px 0;
  color: #35383f;
  line-height: 1.8;
  height: 100px;
  text-align: center;
}
.crumbs {
  padding: 15px 0;
  color: #35383f;
}
.crumbs > ul > li {
  display: inline;
}
.crumbs a {
  color: #35383f;
}
.crumbs a:hover,
.crumbs a.active {
  color: #f58756;
}
.keyword-main {
  border: 1px solid #e5e5e5;
  margin-bottom: 20px;
}
.keyword-main .keyword-item {
  background: #fff;
  position: relative;
  min-height: 40px;
  padding-left: 140px;
  border-bottom: 1px solid #e5e5e5;
}
.keyword-main .keyword-item:last-child {
  border-bottom: none;
}
.keyword-main .keyword-th {
  width: 130px;
  background: #f5f5f5;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
}
.keyword-main .keyword-tit {
  padding: 10px;
  display: block;
  text-align: right;
}
.keyword-main .keyword-list {
  line-height: 1.8;
  padding: 10px;
}
.keyword-main .keyword-list .keyword-li {
  display: inline;
  margin: 0 20px 0 0;
}
.keyword-main .keyword-list .keyword-li a {
  color: #5d5f63;
}
.keyword-main .keyword-list .keyword-li a:hover,
.keyword-main .keyword-list .keyword-li a.active {
  color: #f58756;
}
.product-list {
  font-size: 14px;
  width: 1013px;
}
.product-list .product-item {
  float: left;
  width: 200px;
  height: 300px;
  margin: 0 53px 20px 0;
}
.product-list .product-item-link {
  display: block;
}
.product-list .product-item-img {
  background: url("../images/no-pic-bg.png") no-repeat center #fff;
  border: 1px solid #e5e5e5;
  margin-bottom: 10px;
}
.product-list .product-item-img,
.product-list .product-item-img img {
  width: 198px;
  height: 198px;
}
.product-list .product-item-tit {
  height: 36px;
  line-height: 18px;
  margin-bottom: 10px;
  color: #35383f;
}
.product-list .product-item-price {
  color: #f36f2a;
}
.product-list .product-item-link:hover .product-item-img,
.product-list .product-item-link:active .product-item-img {
  border-color: #f58756;
  box-shadow: 0 0 5px #ddd;
}
.product-det {
  margin-bottom: 60px;
}
.product-det .product-img {
  width: 400px;
}
.product-det .product-big-img {
  margin-bottom: 10px;
  border: 1px solid #e5e5e5;
}
.product-det .product-big-img,
.product-det .product-big-img img {
  width: 398px;
  height: 398px;
}
.product-det .product-img-main {
  height: 80px;
  position: relative;
  padding: 0 23px;
}
.product-det .product-img-prev {
  left: 0;
  background: url("../images/left-arrow-icon.png") no-repeat center #f3f3f3;
}
.product-det .product-img-next {
  right: 0;
  background: url("../images/right-arrow-icon.png") no-repeat center #f3f3f3;
}
.product-det .product-img-prev,
.product-det .product-img-next {
  display: block;
  width: 14px;
  height: 78px;
  line-height: 78px;
  border: 1px solid #e5e5e5;
  text-align: center;
  position: absolute;
  top: 0;
}
.product-det .product-img-prev:hover,
.product-det .product-img-next:hover {
  background-color: #eee;
}
.product-det .product-img-list {
  margin: 0 auto;
  width: 355px;
  height: 80px;
  position: relative;
  overflow: hidden;
}
.product-det .product-img-list > ul > li {
  float: left;
  cursor: pointer;
  margin: 0 4px;
  border: 1px solid #e5e5e5;
}
.product-det .product-img-list > ul > li,
.product-det .product-img-list > ul > li img {
  width: 78px;
  height: 78px;
}
.product-det .product-img-list > ul > li:hover,
.product-det .product-img-list > ul > li.active {
  border-color: #f58756;
  box-shadow: 0 0 5px #ddd;
}
.product-det .product-from {
  width: 500px;
  color: #35383f;
}
.product-det .product-from .product-from-tit,
.product-det .product-from .product-from-price {
  margin-bottom: 20px;
}
.product-det .product-from .product-from-tit {
  font-size: 14px;
  line-height: 1.6;
}
.product-det .product-from .product-from-con,
.product-det .product-from .product-from-item,
.product-det .product-from .product-from-sub-tit {
  margin-bottom: 10px;
}
.product-det .product-from .product-from-sub-tit {
  color: #a3a4a6;
}
.product-det .product-from .product-from-th {
  display: inline-block;
  vertical-align: middle;
  width: 80px;
  *zoom: 1;
  *display: inline;
}
.product-det .product-from .product-inp-text {
  width: 180px;
  height: 24px;
  line-height: 24px;
  padding: 0 5px;
  color: #5d5f63;
  font-size: 12px;
  background: #fff;
  border: 1px solid #e5e5e5;
  border-radius: 2px;
  vertical-align: middle;
}
.product-det .product-from .product-inp-text:focus {
  border-color: #f58756;
}
.product-det .product-from .product-deliver-type {
  margin-right: 20px;
  padding: 0 12px;
  height: 24px;
  line-height: 24px;
  color: #fff;
  border-radius: 2px;
  background: #ff9900;
  border: 1px solid #ff9900;
  display: inline-block;
  vertical-align: middle;
  *zoom: 1;
  *display: inline;
}
.product-det .product-from .product-deliver-type:hover,
.product-det .product-from .product-deliver-type.active {
  background: #ff9900;
  border-color: #ff9900;
}
.product-det .product-from .product-submit-btn {
  width: 120px;
  height: 34px;
  padding: 0 10px;
  text-align: center;
  font-size: 14px;
  color: #fff;
  border-radius: 2px;
  background: #f36f2a;
}
.product-det .product-from .product-submit-btn:hover {
  background: #f6905a;
}
.product-det .product-from .product-submit-btn:active {
  background: #f96d24;
}
