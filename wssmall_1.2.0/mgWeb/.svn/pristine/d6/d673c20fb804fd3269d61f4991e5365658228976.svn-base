.columnTitle h2{ font-weight:bold;}
.remind_div{ background:url(../images/remind_bg.jpg) repeat-x; height:28px; line-height:28px; padding-left:10px;}
.remindico{ background:url(../images/remindico.jpg) no-repeat; display:inline-block; width:14px; height:14px; vertical-align:middle; margin-right:5px;}
.columnMore{ position:absolute; right:5px; top:0px; height:28px; line-height:28px;}
