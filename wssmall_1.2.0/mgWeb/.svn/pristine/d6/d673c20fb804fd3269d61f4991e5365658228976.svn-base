.top_up_con{ border:1px solid #d8d8d8; background:#fff;}
.top_up_search{ height:38px; line-height:38px; border-bottom:1px solid #e4e4e4; padding-left:20px;}
.top_up_ipt{ height:22px; border:1px solid #cdcdcd; vertical-align:middle; color:#a7a7a7;}


.msg_div{ padding-left:150px; position:relative; min-height:150px;}
.msg_pic_div{ position:absolute; left:10px; top:10px; width:120px;}
.msg_pic{ width:120px; height:120px;border:2px solid #c4c4c4; background:#f7f6f6; line-height:120px; text-align:center; color:#a4a4a4;}
.msg_table{ padding-top:20px;}
