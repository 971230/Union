			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>appen_options</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>order_id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>field_desc</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>rows</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>cols</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>field_type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>goodsplugin</description>
		<name>goodsplugin</name>
		<tag-class>com.ztesoft.net.framework.taglib.GoodsPluginTaglib</tag-class>
		<body-content>JSP</body-content>
	</tag>
 	<tag>
		<description>article field tag</description>
		<name>field</name>
		<tag-class>com.ztesoft.net.framework.taglib.ArticleFieldOutTaglib</tag-class>
		<body-content>JSP</body-content>		
	</tag>
		<!-- 翻译es_dc_public_ext的key -->
	<tag>
		<description>dictCode2Name</description>
		<name>dictCode2Name</name>
		<tag-class>com.ztesoft.net.framework.taglib.DictCode2Name</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>attr_code</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>es_dc_public_ext的stype字段</description>
		</attribute>
		<attribute>
			<name>pkey</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>es_dc_public_ext的pkey字段</description>
		</attribute>
	</tag> 
</taglib>
