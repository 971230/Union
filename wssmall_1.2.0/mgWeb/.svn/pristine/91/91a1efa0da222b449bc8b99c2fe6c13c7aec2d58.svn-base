<%@ page contentType="text/html;charset=UTF-8"%>
<%@ include file="/commons/taglibs.jsp"%>
<style>
<!--
.noborder {
	border-style: none;
}
-->
</style>

<div class="input">
	<form class="validate" method="post" id="" validate="true">
		<div>
			<div style="margin-top:5px;">
				<table cellspacing="0" cellpadding="0" border="0" width="100%">
					<tbody>
						<tr style="display: none;">
							<th>日志ID：</th>
							<td><input class="resigterIpt _x_ipt" type="text" name="" id="" value="${detailContent.log_id }" /></td>
						</tr>
						<tr>
							<th>日志名称：</th>
							<td><input class="resigterIpt _x_ipt" type="text" name="" id="" value="${detailContent.dict_name }" /></td>
						</tr>
						<tr style="display: none;">
							<th>本地接口日志ID：</th>
							<td><input class="resigterIpt _x_ipt" type="text" name="" id="" value="${detailContent.local_log_id }" /></td>
						</tr>
						<tr>
							<th>处理建议：</th>
							<td>
								<textarea rows="5" cols="75" name="" id="" value="">${detailContent.deal_opinion }</textarea>
							</td>
						</tr>
						<tr>
							<th>本地接口日志描述：</th>
							<td>
								<textarea rows="8" cols="75" name="" id="" value="">${detailContent.result_desc }</textarea>
							</td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
		<div class="pop_btn">
			<a href="javascript:void(0);" class="blueBtns" id="colsediv" ><span>关闭</span></a>
		</div>
	</form>
</div>