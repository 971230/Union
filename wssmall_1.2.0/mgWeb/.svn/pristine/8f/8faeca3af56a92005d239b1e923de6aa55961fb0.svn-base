<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ include file="/commons/taglibs.jsp"%>
 
<c:forEach var="user" items="${userList}">
       <tr >
           <td><input type="checkbox" name="user.userid"  value="${user.userid }"/></td>
           <td>${user.realname}</td>
           <td>${user.username}</td>
       </tr>
</c:forEach>