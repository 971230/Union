@charset "utf-8";
/* CSS Document */
.hot{ position:absolute; right:-5px; top:2px; background:url(../images/main/hot.png) left top no-repeat; width:27px; height:19px; display:inline-block;}
.movieList{ padding:0px; margin:0px; display:block;}
.movieList li{color:#fff;background:url(../images/black/commentline.gif) left bottom repeat-x; clear:both;}
.movieList li:hover,.movieList li.curr{ background:#3b3b3b url(../images/black/commentline.gif) left bottom repeat-x;}
.moviePic{ position:absolute; left:5px; top:5px;}
.imgDiv{ position:relative;min-height:95px; padding:0px 20px 5px 95px; cursor:pointer;}
.subDiv{ font-size:12px;padding-top:1px; padding-bottom:1px; text-align:left;color:#aba8a8;}
.movieimg{ width:80px; height:80px;}
.movieBg{ position:absolute; bottom:0px; left:0px; right:0px; height:38px; line-height:38px; background:#000; width:150px;filter:alpha(opacity=60); -moz-opacity:0.6; -khtml-opacity: 0.6; opacity: 0.6; }
.movieExplain{height:30px; line-height:30px;font-size:14px;}
.star{ background:url(../images/main/star.png) left top no-repeat; display:inline-block; vertical-align:middle;}
.onestar{ background-position:0px 0px; width:16px; height:16px;}
.twostar{ background-position:0px -16px; width:34px; height:16px;}
.threestar{ background-position:0px -32px; width:52px; height:16px;}
.fourstar{ background-position:0px -48px; width:70px; height:16px;}
.fivestar{ background-position:0px -64px; width:88px; height:16px;}

.columnList{ display:block; padding:0px; margin:0px;}
.columnList li{ padding:0px 8px 5px 0px; float:left;}

.fonts{ font-size:18px;}

