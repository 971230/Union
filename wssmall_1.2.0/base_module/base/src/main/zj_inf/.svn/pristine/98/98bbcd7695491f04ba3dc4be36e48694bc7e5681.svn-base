	private String 	freeAmount;//活动时赠送的金额，单位厘
	private String 	cardType;//卡类型  普卡，微卡，nano卡
	private String 	goodsQuantity;//商品数量
	private String 	frzMoney;//特殊活动的冻结金额 单位厘
