<!DOCTYPE html>
<html>
<head>
<title>电商平台 - API详情</title>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="shortcut icon" href="" type="image/x-icon">
<link rel="stylesheet" href="css/base-min.css">
<link rel="stylesheet" href="css/common.css">
<link rel="stylesheet" href="css/error.css">
<link rel="stylesheet" href="css/open-base.css">
<link rel="stylesheet" href="css/sh.css">
<link rel="stylesheet" href="css/editor-pkg-min-datauri.css" >
<link rel="stylesheet" href="css/right-tools.css">
<script src="js/jquery-1.4.2.min.js"></script>
</head>
<body>
	<div class="top-header-menu">
		<div class="top-menu-cont">
			<h1 class="top-logo" style="display:none;">
				<a class="top-logo-link" href="http://open.taobao.com/" alt="淘宝开放平台"></a>
			</h1>
			<!-- 搜索start -->
			<div class="search-wrap">
				<div class="search">
					<div id="search">
						<div class="default"><span class="initText">请输入您要搜索的内容</span></div>
						<div class="actived">
							<input autocomplete="off" class="searchText" id="searchText" t="api" onkeypress="EnterPress()" onkeydown="EnterPress(event)" type="text"> 
							<a class="sBtn" onclick="searchapi()"></a>
						</div>
					</div>
				</div>
			</div>
			<!-- 搜索end -->
			<ul class="top-nav-menu clearfix">
				<li><a href="#">文档</a></li>
				<li class="selected"><a href="index.html">API<iclass="reddot"></i></a></li>
			</ul>
		</div>
	</div>
	<!-- header end -->

	<!--header end-->
	<!--面包屑 begin-->
	<div class="crumbs">
		<a href="index.html">用户API</a>
		<a class="last" href="#"><#if classname??>${classname}</#if></a>
		<!--class with "last"-->
	</div>
	<!--面包屑 end-->

	<div class="container grid-s165m0 layout">
		<!--正文区 begin-->
		<div class="col-main bg_line">
			<div class="main-wrap">
				<div class="title-wrap">
					<h1 class="open-bg title">
						<#if classname??>${classname}</#if> <#if desc??>${desc}</#if> 
					</h1>

					<ul class="api-sub-title layout clearfix">
						<li class="sub-title"><a class="link" href="#param-in-h2">函数输入参数</a></li>
						<li class="sub-title last"><a class="link" href="#param-out-h2">函数输出参数</a></li>
					</ul>
				</div>

				<div class="api-detail-bd" id="bd">
					<div>
						<p class="title-intro"><#if summary??>${summary}</#if></p>
						<!-- 函数入参<<begin -->
						<div>
							<h2 class="head-title" id="param-in-h2">
								<img src="image/tri_down.png" style="cursor:pointer;margin-right:5px;" onclick="changeHide(this,'param-in')"> 函数输入参数<font color="blue"><b>${parain}</b></font>
							</h2>
							<div id="param-in" style="display:">
								<table class="api-table" cellspacing="0">
									<thead>
										<tr>
											<th class="middle" width="15%">名称</th>
											<th class="middle" width="15%">变量名</th>
											<th class="middle" width="15%">类型</th>
											<th class="middle" width="15%">是否为空</th>
											<th class="left" width="">说明</th>
										</tr>
									</thead>
									<tbody>
										<#if methodplist??>
										<#list methodplist as paramin>
										<tr class="<#if (paramin_index%2==1)>even</#if><#if (paramin_index%2==0)>odd</#if>">
											<td class="middle">${paramin.name}</td>
											<td class="middle"><font color='red'>${paramin.fieldname}</font></td>
											<td class="middle">${paramin.type}</td>
											<td class="middle">${paramin.isnecessary}</td>
											<td class="left">${paramin.desc}</td>
										</tr>
										</#list>
										</#if>										
									</tbody>
								</table>
							</div>
						</div>
						<!-- 函数入参>>end -->
						<!-- 函数出参<<begin -->
						<div>
							<h2 class="head-title" id="param-out-h2">
								<img src="image/tri_down.png" style="cursor:pointer;margin-right:5px;" onclick="changeHide(this,'param-out')"> 函数输出参<font color="blue"><b>${paraout}</b></font>
							</h2>
							<div id="param-out" style="display:">
								<table class="api-table" cellspacing="0">
									<thead>
										<tr>
											<th class="middle" width="15%">名称</th>
											<th class="middle" width="15%">变量名</th>
											<th class="middle" width="15%">类型</th>
											<th class="middle" width="15%">是否为空</th>
											<th class="left" width="">说明</th>										
										</tr>
									</thead>
									<tbody>
										<#if methodrlist??>
										<#list methodrlist as paramout>						
										<tr class="<#if (paramout_index%2==1)>even</#if><#if (paramout_index%2==0)>odd</#if>">
											<td class="middle">${paramout.name}</td>
											<td class="middle"><font color='red'>${paramout.fieldname}</font></td>
											<td class="middle">${paramout.type}</td>
											<td class="middle">${paramout.isnecessary}</td>
											<td class="left">${paramout.desc}</td>
										</tr>
										</#list>	
										</#if>
									</tbody>
								</table>
							</div>
						</div>
						<!-- 函数出参>>end -->						
					</div>
					
				</div>
			</div>
		</div>
		<!--正文区 end-->
		<!--左侧导航栏 begin-->
		<div class="col-sub">
			<div class="APIgory">
				<ul id="leftnavigation">
					<li>
						<div class="APIgory-title">
							<a href="index.html">返回&nbsp;<span class="APIgory-sub-title">${classname}</span></a>
						</div>
					</li>
					<#list methodlist as ml>
						<li url="${ml.fullmethodname}.html">
							<div class="APIgory-list <#if (ml.methodplist?exists)||(ml.methodrlist?exists)>focus</#if> ">
								<div class="APIgory-content">
									<a class="link APIgoryItem title-overflow" href="${ml.fullmethodname}.html" title="${ml.methodname}">${ml.fullmethodname}</a>
									<p class="APIgory-intro info-overflow">${ml.desc}</p>
								</div>
								<s></s>
							</div>
						</li>
				  </#list>
				</ul>
			</div>
		</div>
		<!--左侧导航栏 end-->
	</div>

	<script type="text/javascript">
		function changeHide(imgDom, contentDivId) {
			var contentDiv = document.getElementById(contentDivId);
			if (contentDiv.style.display == "none") {
				contentDiv.style.display = "";
				imgDom.src = "image/tri_down.png";
			} else {
				contentDiv.style.display = "none";
				imgDom.src = "image/tri_right.png";
			}
		}
		
		$(function(){
				$($("#leftnavigation").find("li")).each(function(){
					$(this).bind("click",function(){
						 window.location.href=$(this).attr("url");
					});	
					
				});
		
		})
	</script>
	<!--footer begin-->
	<div id="footer">
		<div class="footer">
			<div class="contact">
				<a class="weibo" href="#" target="_blank">电商平台</a> 
			</div>

			<div class="about">
				<span class="copyright">Copyright ? 2003-2012 ZteSoft 版权所有</span>
				<span class="links">
				 <a href="#" target="_blank">关于我们</a>
				</span>
			</div>
		</div>
	</div>
</body>
</html>