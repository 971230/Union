package com.ztesoft.net.template.model;

import java.io.Serializable;
import java.util.Map;

public class NodeModel implements Serializable{
	
	private static final long serialVersionUID = 1659667617132962979L;
	
	private Map<String, NodeModel> sub_node;
	
	private OrderTemplateNode nodeInfo;
	
	private OrderTemplateNode pNodeInfo;

	public Map<String, NodeModel> getSub_node() {
		return sub_node;
	}

	public void setSub_node(Map<String, NodeModel> sub_node) {
		this.sub_node = sub_node;
	}

	public OrderTemplateNode getNodeInfo() {
		return nodeInfo;
	}

	public void setNodeInfo(OrderTemplateNode nodeInfo) {
		this.nodeInfo = nodeInfo;
	}

	public OrderTemplateNode getpNodeInfo() {
		return pNodeInfo;
	}

	public void setpNodeInfo(OrderTemplateNode pNodeInfo) {
		this.pNodeInfo = pNodeInfo;
	}
	
}
