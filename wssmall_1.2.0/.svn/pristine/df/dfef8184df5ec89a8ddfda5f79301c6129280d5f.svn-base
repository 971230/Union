        <servlet-name>wxw</servlet-name>
        <servlet-class>com.ztesoft.wx.servlet.WXServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wxw</servlet-name>
        <url-pattern>/wxw.action</url-pattern>
