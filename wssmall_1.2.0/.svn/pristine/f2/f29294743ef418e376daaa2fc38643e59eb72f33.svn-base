package com.ztesoft.rule.core.module.fact;


/**
 * 规则结果集：记录每个rule执行结果;如果规则出错，则存储到Error中
 * @author easonwu 
 * @creation Dec 17, 2013
 * 
 */
public class RuleResult {
	//规则编码 
	private String ruleCode ;
	//是规则ID
	private String rule_id;
	//结果值
	private Object result ;
	
	public RuleResult(){
		
	}
	
	public RuleResult(String ruleCode , Object result){
		this.ruleCode = ruleCode ;
		this.result = result ;
	}
	
	public RuleResult(String ruleCode, String rule_id, Object result) {
		this.ruleCode = ruleCode;
		this.rule_id = rule_id;
		this.result = result;
	}

	public String getRuleCode() {
		return ruleCode;
	}
	public void setRuleCode(String ruleCode) {
		this.ruleCode = ruleCode;
	}
	public Object getResult() {
		return result;
	}
	public void setResult(Object result) {
		this.result = result;
	}

	public String getRule_id() {
		return rule_id;
	}

	public void setRule_id(String rule_id) {
		this.rule_id = rule_id;
	}
	
	
}
