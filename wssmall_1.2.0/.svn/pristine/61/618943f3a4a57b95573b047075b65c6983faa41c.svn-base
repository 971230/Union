package com.ztesoft.net.mall.core.model;

import java.io.Serializable;

/**
 * 订单使用规格表
* @作者 MoChunrun 
* @创建日期 2013-10-9 
* @版本 V 1.0
 */
public class OrderUseDef implements Serializable {

	private String use_id;
	/**
	 * --1确认工号
		--2发货工号
		--3受理工号
		--4支付工号
	 */
	private String use_tye;
	private String es_order_field_name;
	public String getUse_id() {
		return use_id;
	}
	public void setUse_id(String use_id) {
		this.use_id = use_id;
	}
	public String getUse_tye() {
		return use_tye;
	}
	public void setUse_tye(String use_tye) {
		this.use_tye = use_tye;
	}
	public String getEs_order_field_name() {
		return es_order_field_name;
	}
	public void setEs_order_field_name(String es_order_field_name) {
		this.es_order_field_name = es_order_field_name;
	}
	
}
