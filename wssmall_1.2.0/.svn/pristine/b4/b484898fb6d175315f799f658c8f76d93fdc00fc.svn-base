package com.ztesoft.cms.page.vo;

import com.ztesoft.ibss.common.util.Const;

import java.util.HashMap;

/**
 * @author Reason.Yea
 * @version Created Oct 25, 2012
 */
public class CmsFun {
	
	private String fun_id;
	private String fun_name;
	private String fun_type;
	private String self_rule;
	private String cache_flag;
	private String file_id;
	private String show_cols;
	private String edit_cols;
	private String state;
	private String visit_url;
	private String oper_staff_no;
	private String edit_page;
	
	public CmsFun(HashMap map){
		this.fun_id = Const.getStrValue(map, "fun_id");
		this.fun_name = Const.getStrValue(map, "fun_name");
		this.fun_type = Const.getStrValue(map, "fun_type");
		this.self_rule = Const.getStrValue(map, "self_rule");
		this.cache_flag = Const.getStrValue(map, "cache_flag");
		this.file_id = Const.getStrValue(map, "file_id");
		this.show_cols = Const.getStrValue(map, "show_cols");
		this.edit_cols = Const.getStrValue(map, "edit_cols");
		this.state = Const.getStrValue(map, "state");
		this.visit_url = Const.getStrValue(map, "visit_url");
		this.oper_staff_no = Const.getStrValue(map, "oper_staff_no");
		this.edit_page = Const.getStrValue(map, "edit_page");
	}
	
	public String getFun_id() {
		return fun_id;
	}
	public void setFun_id(String fun_id) {
		this.fun_id = fun_id;
	}
	public String getFun_name() {
		return fun_name;
	}
	public void setFun_name(String fun_name) {
		this.fun_name = fun_name;
	}
	public String getFun_type() {
		return fun_type;
	}
	public void setFun_type(String fun_type) {
		this.fun_type = fun_type;
	}
	public String getSelf_rule() {
		return self_rule;
	}
	public void setSelf_rule(String self_rule) {
		this.self_rule = self_rule;
	}
	public String getCache_flag() {
		return cache_flag;
	}
	public void setCache_flag(String cache_flag) {
		this.cache_flag = cache_flag;
	}
	public String getFile_id() {
		return file_id;
	}
	public void setFile_id(String file_id) {
		this.file_id = file_id;
	}
	public String getShow_cols() {
		return show_cols;
	}
	public void setShow_cols(String show_cols) {
		this.show_cols = show_cols;
	}
	public String getEdit_cols() {
		return edit_cols;
	}
	public void setEdit_cols(String edit_cols) {
		this.edit_cols = edit_cols;
	}
	public String getState() {
		return state;
	}
	public void setState(String state) {
		this.state = state;
	}
	public String getVisit_url() {
		return visit_url;
	}
	public void setVisit_url(String visit_url) {
		this.visit_url = visit_url;
	}
	public String getOper_staff_no() {
		return oper_staff_no;
	}
	public void setOper_staff_no(String oper_staff_no) {
		this.oper_staff_no = oper_staff_no;
	}
	public String getEdit_page() {
		return edit_page;
	}
	public void setEdit_page(String edit_page) {
		this.edit_page = edit_page;
	}
	
}
