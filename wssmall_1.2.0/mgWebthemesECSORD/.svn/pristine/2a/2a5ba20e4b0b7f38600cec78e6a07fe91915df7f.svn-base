.sgreenbtn, .sgreenbtn:hover, .sgreenbtn span, .sgreenbtn:hover span{ background:url(../images/greenbtn.gif) no-repeat;display:inline-block; height:22px; line-height:22px;padding:0 0 0 4px; color:#fff;  margin:0; color:#fff; font-size:12px;font-size:12px;}
.sgreenbtn{ background-position:0px 0px;}
