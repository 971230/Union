@charset "utf-8";
/* CSS Document */
body{ min-width:1003px; font-family:Arial, Helvetica, sans-serif; background:url(../images/mainbg.gif) repeat-y;}

/*顶部样式*/
.topBar{ background:url(../images/topbar.gif) repeat-x; height:31px;}
.topBarCon{ width:1003px; margin:0 auto; height:31px; line-height:31px;}
.topBarConL{ width:300px; float:left;color:#666;}
.topBarConR{ width:600px; float:right;color:#666; text-align:right;}
.topBarConL a{ color:#666;}
.topBarConL a:hover{ color:#649300; text-decoration:underline;}
.col{ background:url(../images/col.gif) left top no-repeat; width:10px; height:10px; display:inline-block; margin-right:3px;}
.topBarConR span{ margin-left:5px;}
.topBarConR span.wel{ margin-right:40px;}
.topBarConR span a{ color:#666;}
.topBarConR span a:hover{ color:#649300; text-decoration:underline;}

/*页面共用DIV*/
.container{ width:1003px; margin:0 auto;} 
.mainDiv{ width:100%; margin:0 auto; position:relative;}

/*LOGO样式*/
.topbg{ background:url(../images/topbar.jpg) left top no-repeat; height:67px;}
.logoDiv{ height:67px; background:url(../images/logo.jpg) no-repeat;}
.advertDiv{ height:47px; text-align:center; width:304px; height:30px; position:absolute; right:20px; top:0px;background:url(../images/advertbg.jpg) no-repeat;}
  
/*导航栏样式*/
.naviDiv{position:relative; padding-left:250px;}
.naviLeft{height:67px;float:left;}
.naviRight{ width:125px; height:67px; text-align:center;position:absolute; top:0px;  right:0px;}
.navi{ height:32px; margin-top:35px;}
.navL{ height:32px;}
.nav{ padding:0px; margin:0px; display:block;}
.nav li{ float:left;height:32px; line-height:32px; text-align:center;}
.nav li.last{ background:none;}
.nav li a{ display:inline-block;color:#fff; font-size:12px;cursor:pointer;padding:0 0 0 6px; background:url(../images/navl.png) no-repeat;}
.nav li a span{ display:inline-block;padding:0 15px 0 10px; background:url(../images/navr.png) right top no-repeat;}
.nav li a:hover, .nav li a.curr{ background:url(../images/navhoverl.png) left top no-repeat; color:#003c73;}
.nav li a:hover span, .nav li a.curr span{ background:url(../images/navhoverr.png) right top no-repeat;padding:0 15px 0 10px;}
.graybtn, .graybtn span, .graybtn:hover span{ background:url(../images/loginbtn.gif) no-repeat; display:inline-block; height:26px; line-height:26px;padding:0 0 0 3px; color:#fff;  margin:0; color:#666; font-weight:bold;font-size:12px;}
.graybtn{ background-position:0px 0px;}
.graybtn span{background-position:right -26px;padding:0 5px 0 5px;min-width:32px;text-align:center; margin:0;}
.graybtn:hover{ background-position:0px -52px;}
.graybtn:hover span{background-position:right -78px;padding:0 5px 0 5px;}
.homeicon{ background:url(../images/home.gif) left top no-repeat; width:13px; height:15px; display:inline-block; vertical-align:-3px;margin-right:3px;}
.exiticon{ background:url(../images/exit.gif) left top no-repeat; width:13px; height:15px; display:inline-block; vertical-align:-3px;margin-right:3px;}

/*页面主体布局*/
.mainContent{ padding-left:155px;background:url(../images/mainbg.gif) repeat-y;}
.leftDiv{ width:155px; position:absolute; left:0px; /* top:67px; */}
.rightDiv{ }
.mart10{ margin-top:10px;}
.leftMenuDiv{border:1px solid #cecece; border-top:none;}
.leftTop{ background:url(../images/lefttop.gif) repeat-x;width:153px; height:28px; position:relative;border-bottom:1px solid #d3d3d3;border-top:1px solid #dadde0;}
.leftTop h2{ color:#585858; font-weight:bold; line-height:28px; font-size:12px; padding-left:30px;}
.leftArrow{ position:absolute; left:10px; top:50%; margin-top:-6px;}
.arrowclose{ background:url(../images/arrowclose.gif) left top no-repeat; width:12px; height:12px; display:inline-block;}
.arrowopen{ background:url(../images/arrowopen.gif) left top no-repeat; width:15px; height:15px; display:inline-block;}
.leftMenu{}
.leftMenu li{border-bottom:1px solid #eaeaea;}
.leftMenu li a{ color:#444; display:block; padding-left:20px; font-size:12px; height:30px; line-height:30px; background:#fff url(../images/leftarrow.gif) right center no-repeat;}
.leftMenu li a:hover,.leftMenu li a.curr{ background:#46acec url(../images/leftarrow_curr.gif) right center no-repeat; color:#fff;}
.leftMenu li.last{ border-bottom:none;}

/*表格样式*/
.grid{ margin-top:5px;}
.grid table{ border-collapse:collapse; border-top:1px solid #cdcdcd;border-right:1px solid #cdcdcd; font-family:Arial, Helvetica, sans-serif;}
.grid table th{ background:#fff; height:30px; line-height:30px; border-bottom:1px solid #cdcdcd; border-top:1px solid #cdcdcd; text-align:center; font-weight:bold; color:#434343;}
.grid table td{ border-bottom:1px dashed #e3e3e3; height:30px; vertical-align:middle; color:#666; text-align:center;}
.grid table td a{ color:#00469d; text-decoration:none;}
.grid table td span.sper{ margin-left:3px; margin-right:3px;}
.grid table tr:hover{ cursor:pointer; background:#f0f0f0;}
.grid table td span.red_font{ color:#df3232;}

.comBtnDiv{background:url(../images/combtndiv.gif) repeat-x; height:26px; padding-top:4px; padding-left:5px;}
.comBtn{ background:url(../images/btn_cms.gif) left top repeat-x; border:1px solid #a1d561;  height:24px; line-height:24px;color:#444; text-align:center; cursor:pointer; display:inline-block; padding-left:15px; padding-right:15px;}
.comBtn:hover{ background:url(../images/btn_hover.gif) left top repeat-x; color:#444;}
.comBtnDisable{ background:url(../images/01.png) left top repeat-x; cursor:pointer; border:1px solid #bbbbbb; text-align:center;padding-left:15px; padding-right:15px;height:22px;line-height:22px; display:inline-block;color:#c5c5c5; }

.graybtn1{ background:url(../images/btn.gif) 0px -22px repeat-x; display:inline-block; height:22px;border:1px solid #89b6d9;padding:0 8px 0 8px;  margin:0;font-size:12px; color:#444; text-align:center;}
.graybtn1 span{}
.graybtn1:hover{ background:url(../images/btn_hover.gif) repeat-x;}

/*列表tab标签效果*/
.stat_graph{ margin-top:5px;}
.stat_graph h3 { background: url(../images/graph_tab.gif) repeat-x scroll 0 0 transparent; height: 28px;}
.graph_tab{height:28px; padding-left:10px;}
.graph_tab li{float:left;display:inline;margin-right:3px;padding-left:10px;color:#000;font-size:12px;cursor:pointer;height:28px;line-height:28px;border:1px solid #d5d5d5;border-bottom:none;border-top:none;background:url(../images/tabli.gif) repeat-x;}
.graph_tab li span.word{float:left;}
.graph_tab li span.bg{float:left;display:block;width:10px;height:28px;}
.graph_tab li.selected{float:left;background:url(../images/tabselected.gif) repeat-x;height:28px;}
.graph_tab li.selected span.bg{float:left;display:block;width:10px;height:27px;}

/*表单tab标签效果*/
.stat_form{}
.form_tab{}

/*form表单样式*/
.searchformDiv{ background:#f2f2f2; border-bottom:1px solid #cdcdcd;border-right:1px solid #cdcdcd; border-top:1px solid #f9f9f9; padding:5px;}
.searchformDiv table{}
.searchformDiv table th{ text-align:right; padding-right:3px; vertical-align:middle; color:#666; width:85px;}
.searchformDiv table td{ padding-top:3px; padding-bottom:3px; vertical-align:middle;}
.searchipt{ border:1px solid #cecece; background:#fff; height:22px; line-height:22px; color:#666; width:150px; padding-left:3px;}

/*用户信息样式*/
.userMsgDiv{ background:#f9f9f9; border:1px solid #d3d3d3; padding:5px; color:#666; line-height:24px; }
.userImgDiv{ width:150px; float:left; text-align:center; padding-top:10px; padding-bottom:10px;}
.usrImg{ border:1px solid #d3d3d3; background:#fff; padding:2px;}
.userImgDiv a{ color:#666; text-decoration:underline;}
.userMsg{ width:350px; float:left;padding-top:8px;}
.loginTime{ width:250px; float:right;}
.loginTime p.signIn{ text-align:center; padding-top:10px; padding-bottom:10px;}

/*订单详情样式*/
.grayBox{ margin-top:10px;border:1px solid #d0d0d0;}
.grayBoxTitle{background:#f4f4f4; height:24px; line-height:24px; color:#333; font-weight:bold; padding-left:10px;}
.grayBox table{ color:#666;}
.grayBox table th{ width:180px; padding-left:5px; border-right:1px solid #e9e9e9; border-top:1px solid #e9e9e9;}
.grayBox table td{ height:24px; line-height:24px; border-right:1px solid #e9e9e9; border-top:1px solid #e9e9e9; padding-left:5px; font-family:Arial, Helvetica, sans-serif;width:35%;}

/*产品列表样式*/
.proCon{ margin-top:10px; border:1px solid #d4d4d4; border-top:none;}
.proTitle{ background:url(../images/proTitle.gif) repeat-x; height:31px; position:relative;}
.proTitle h2{ color:#666; font-weight:bold; line-height:33px; padding-left:5px;}
.proList{ display:block; padding:0px; margin:0px;}
.proList li{ width:24.5%; height:250px; float:left; border-right:1px solid #e7e6e6; border-bottom:1px solid #e7e6e6;}
.proList li.last{ border-right:none;}
.proList li.back{ border-bottom:none;}
.proDiv{ width:205px; margin:0 auto; line-height:18px; color:#666;}
.proDiv p.img{ text-align:center; padding-top:10px;}
.proDiv p.title{ text-align:center; color:#333; font-weight:bold; padding-top:3px; padding-bottom:3px;}
.proDiv p.txt{ padding-left:5px; padding-right:5px;}
.proDiv p.btnDiv{ text-align:center; padding-top:5px; padding-bottom:5px;}
.buybtn, .buybtn span, .buybtn:hover, .buybtn:hover span{ background:url(../images/buybtn.gif) no-repeat;display:inline-block; height:30px; line-height:30px;padding:0 0 0 3px; color:#fff;  margin:0; color:#fff; font-size:12px;}
.buybtn{ background-position:0px 0px;}
.buybtn span{background-position:right -30px;padding:0 10px 0 8px;min-width:70px;text-align:center; margin:0;}
.buybtn:hover{ background-position:0px -60px;}
.buybtn:hover span{background-position:right -90px;padding:0 10px 0 8px;}

.greenbtn, .greenbtn:hover, .greenbtn span, .greenbtn:hover span{ background:url(../images/buybtn.gif) no-repeat;display:inline-block; height:35px; line-height:35px;padding:0 0 0 17px; color:#fff;  margin:0; color:#fff; font-size:12px; font-family:"微软雅黑"; font-size:14px; font-weight:bold;}
.greenbtn{ background-position:0px -120px;}
.greenbtn span{background-position:right -155px;padding:0 25px 0 8px;min-width:50px;text-align:center; margin:0;}
.greenbtn:hover{ background-position:0px -190px;}
.greenbtn:hover span{background-position:right -225px;padding:0 25px 0 8px;}

 
.topbtn1, .topbtn1 span{background:url(../images/allbtn.gif) no-repeat; display:inline-block;  background-position:0px 0px; height:30px; line-height:30px;padding:0 0 0 3px; color:#666; margin-right:5px;}
.topbtn1 strong{ color:#00469d; font-weight:bold;}
.topbtn1 span{ background-position:right -30px; padding:0 6px 0 3px; display:inline-block;}
.topbtn2, .topbtn2 span, .topbtn2:hover, .topbtn2:hover span, .topbtn2.curr, .topbtn2.curr span{ display:inline-block;height:28px; line-height:28px;padding:0 0 0 8px; color:#004a77;}
.topbtn2{color:#444;}
.topbtn2 strong{ color:#00469d; font-weight:bold;}
.topbtn2 span{padding:0 5px 0 2px; display:inline-block;}
.topbtn2:hover, .topbtn2.curr{ background:url(../images/bluebtnl.png) left top no-repeat;}
.topbtn2:hover span, .topbtn2.curr span{padding:0 5px 0 2px; display:inline-block;background:url(../images/bluebtnr.png) right top no-repeat; color:#fff;}
.topbtn3, .topbtn3 span{ display:inline-block;height:28px; line-height:28px;padding:0 0 0 3px; color:#fff;}
.topbtn3{color:#fff; background:url(../images/bluebtnl.png) left top no-repeat; padding-left:10px;}
.topbtn3 span{padding:0 10px 0 0px; display:inline-block; background:url(../images/bluebtnr.png) right top no-repeat;}
.allicon{display:inline-block; width:18px; height:18px; vertical-align:middle; margin-right:3px;}
.msgicon{ background:url(../images/topico04.png) center center no-repeat;}
.deskbook{ background:url(../images/topico01.png) center center no-repeat;}
.appicon{ background:url(../images/topico05.png) center center no-repeat;}
.collect{ background:url(../images/topico02.png) center center no-repeat;}
.exiticon{ background:url(../images/topico03.png) center center no-repeat;}
.searchBtn{ background:url(../images/searchbtn.gif) repeat-x; height:22px; line-height:22px; border:1px solid #023752; display:inline-block; color:#fff; padding:0 12px 0 12px; font-size:12px;}
.searchBtn span{}
.searchBtn:hover{ background:url(../images/searchbtn_hover.gif) repeat-x;}
.searchBtn:hover span{ }

/*表单样式*/
.fieldset{}
.legend{}
.legend h2{ background:#eefce2;}

.distributorL{ width:40%; float:left;}
.distributorR{ width:60%; float:left;}


