.submitbtn{ background:url(../images/submitbtn.gif) repeat-x; height:22px; line-height:22px; color:#fff; border:1px solid #023752; display:inline-block; padding-left:15px; padding-right:15px; margin-left:3px; margin-right:3px;}
.resetbtn{background:url(../images/resetbtn.gif) repeat-x; height:22px; line-height:22px; color:#444; border:1px solid #a4a4a4; display:inline-block; padding-left:15px; padding-right:15px; margin-left:3px; margin-right:3px;}
.formgrid{}
