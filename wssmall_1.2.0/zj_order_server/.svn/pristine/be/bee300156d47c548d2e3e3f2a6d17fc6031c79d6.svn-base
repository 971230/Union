package com.ztesoft.net.model;

import java.io.Serializable;

import org.apache.commons.lang.StringUtils;

public class InvoiceModeFieldVO implements Serializable{
	private String id;
	private String model_cd;
	private String field_cd;
	private String print_mode;
	private String pos_x;
	private String pos_y;
	private String row_num;
	private String font_name;
	private String font_style;
	private String font_size;
	private String source_from;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getModel_cd() {
		return model_cd;
	}
	public void setModel_cd(String model_cd) {
		this.model_cd = model_cd;
	}
	public String getField_cd() {
		return field_cd;
	}
	public void setField_cd(String field_cd) {
		this.field_cd = field_cd;
	}
	public String getPrint_mode() {
		return print_mode;
	}
	public void setPrint_mode(String print_mode) {
		this.print_mode = print_mode;
	}
	public String getPos_x() {
		if (StringUtils.isBlank(pos_x))
			return "0";
		return pos_x;
	}
	public void setPos_x(String pos_x) {
		this.pos_x = pos_x;
	}
	public String getPos_y() {
		if (StringUtils.isBlank(pos_y))
			return "0";
		return pos_y;
	}
	public void setPos_y(String pos_y) {
		this.pos_y = pos_y;
	}
	public String getRow_num() {
		return row_num;
	}
	public void setRow_num(String row_num) {
		this.row_num = row_num;
	}
	public String getFont_name() {
		return font_name;
	}
	public void setFont_name(String font_name) {
		this.font_name = font_name;
	}
	public String getFont_style() {
		return font_style;
	}
	public void setFont_style(String font_style) {
		this.font_style = font_style;
	}
	public String getFont_size() {
		return font_size;
	}
	public void setFont_size(String font_size) {
		this.font_size = font_size;
	}
	public String getSource_from() {
		return source_from;
	}
	public void setSource_from(String source_from) {
		this.source_from = source_from;
	}
	
}
