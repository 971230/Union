package com.ztesoft.net.ecsord.params.ecaop.vo;

import java.io.Serializable;

import com.ztesoft.net.annotation.ZteSoftCommentAnnotationParam;

public class PhotoInfoVO implements Serializable {

	@ZteSoftCommentAnnotationParam(name="图片序号",type="String",isNecessary="Y",desc="photo_id：图片序号")
	private String photo_id;
	
	@ZteSoftCommentAnnotationParam(name="action",type="String",isNecessary="Y",desc="action：动作A:新增 M:更新")
	private String action;
	
	@ZteSoftCommentAnnotationParam(name="photo_stream",type="String",isNecessary="Y",desc="photo_stream：图片流")
	private String photo_stream;

	public String getPhoto_id() {
		return photo_id;
	}

	public void setPhoto_id(String photo_id) {
		this.photo_id = photo_id;
	}

	public String getAction() {
		return action;
	}

	public void setAction(String action) {
		this.action = action;
	}

	public String getPhoto_stream() {
		return photo_stream;
	}

	public void setPhoto_stream(String photo_stream) {
		this.photo_stream = photo_stream;
	}

}
