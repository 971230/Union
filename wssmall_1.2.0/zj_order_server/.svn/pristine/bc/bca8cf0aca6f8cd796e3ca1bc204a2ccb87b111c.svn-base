package com.ztesoft.net.ecsord.params.ecaop.vo;

import java.io.Serializable;

import com.ztesoft.net.annotation.ZteSoftCommentAnnotationParam;

/**
 * delivery_info节点 （物流信息/收货人信息）
 * @author song.qi
 *2017年12月26日
 */
public class DeliveryInfo implements Serializable {

	private static final long serialVersionUID = 1L;

	@ZteSoftCommentAnnotationParam(name = "deli_mode", type = "String", isNecessary = "N", desc = "配送方式")
	private String deli_mode;

	@ZteSoftCommentAnnotationParam(name = "deli_time_mode", type = "String", isNecessary = "N", desc = "配送时间类型")
	private String deli_time_mode;//配送时间类型，自提时非必填

	@ZteSoftCommentAnnotationParam(name = "deli_name", type = "String", isNecessary = "N", desc = "收货人姓名")
	private String deli_name;

	@ZteSoftCommentAnnotationParam(name = "deli_phone", type = "String", isNecessary = "N", desc = "收货人电话")
	private String deli_phone;

	@ZteSoftCommentAnnotationParam(name = "deli_province", type = "String", isNecessary = "N", desc = "收货省")
	private String deli_province;//标准6位编码

	@ZteSoftCommentAnnotationParam(name = "deli_city", type = "String", isNecessary = "N", desc = "收货地市")
	private String deli_city;//标准6位编码

	@ZteSoftCommentAnnotationParam(name = "deli_district", type = "String", isNecessary = "N", desc = "收货区县")
	private String deli_district;//标准6位编码
	
	@ZteSoftCommentAnnotationParam(name = "deli_code", type = "String", isNecessary = "N", desc = "收货人邮编")
	private String deli_code;//收货人邮编
	
	@ZteSoftCommentAnnotationParam(name = "deli_addr", type = "String", isNecessary = "N", desc = "收货地址")
	private String deli_addr;//收货地址
	
	@ZteSoftCommentAnnotationParam(name = "shipping_company", type = "String", isNecessary = "N", desc = "物流公司")
	private String shipping_company;//物流公司
	
	@ZteSoftCommentAnnotationParam(name = "logi_no", type = "String", isNecessary = "N", desc = "物流单号")
	private String logi_no;//物流单号

	public String getDeli_mode() {
		return deli_mode;
	}

	public void setDeli_mode(String deli_mode) {
		this.deli_mode = deli_mode;
	}

	public String getDeli_time_mode() {
		return deli_time_mode;
	}

	public void setDeli_time_mode(String deli_time_mode) {
		this.deli_time_mode = deli_time_mode;
	}

	public String getDeli_name() {
		return deli_name;
	}

	public void setDeli_name(String deli_name) {
		this.deli_name = deli_name;
	}

	public String getDeli_phone() {
		return deli_phone;
	}

	public void setDeli_phone(String deli_phone) {
		this.deli_phone = deli_phone;
	}

	public String getDeli_province() {
		return deli_province;
	}

	public void setDeli_province(String deli_province) {
		this.deli_province = deli_province;
	}

	public String getDeli_city() {
		return deli_city;
	}

	public void setDeli_city(String deli_city) {
		this.deli_city = deli_city;
	}

	public String getDeli_district() {
		return deli_district;
	}

	public void setDeli_district(String deli_district) {
		this.deli_district = deli_district;
	}

	public String getDeli_code() {
		return deli_code;
	}

	public void setDeli_code(String deli_code) {
		this.deli_code = deli_code;
	}

	public String getDeli_addr() {
		return deli_addr;
	}

	public void setDeli_addr(String deli_addr) {
		this.deli_addr = deli_addr;
	}

	public String getShipping_company() {
		return shipping_company;
	}

	public void setShipping_company(String shipping_company) {
		this.shipping_company = shipping_company;
	}

	public String getLogi_no() {
		return logi_no;
	}

	public void setLogi_no(String logi_no) {
		this.logi_no = logi_no;
	}


}
