package com.ztesoft.net.ecsord.params.ecaop.vo;

import java.io.Serializable;

import com.ztesoft.net.annotation.ZteSoftCommentAnnotationParam;

/**
 * 2.3.5. 订单信息查询接口
 * 
 * @author 宋琪
 *
 * @date 2017年6月14日
 */
public class OrderDeliveryInfo implements Serializable {

	@ZteSoftCommentAnnotationParam(name = "ship_name", type = "String", isNecessary = "N", desc = "ship_name：收货人姓名")
	private String ship_name;

	@ZteSoftCommentAnnotationParam(name = "ship_province", type = "String", isNecessary = "N", desc = "ship_province：收货省")
	private String ship_province;

	@ZteSoftCommentAnnotationParam(name = "ship_city", type = "String", isNecessary = "N", desc = "ship_city：收货地市")
	private String ship_city;

	@ZteSoftCommentAnnotationParam(name = "ship_district", type = "String", isNecessary = "N", desc = "ship_district：收货区县")
	private String ship_district;

	@ZteSoftCommentAnnotationParam(name = "ship_addr", type = "String", isNecessary = "N", desc = "ship_addr：收货地址")
	private String ship_addr;

	@ZteSoftCommentAnnotationParam(name = "shipping_company", type = "String", isNecessary = "N", desc = "shipping_company：物流公司")
	private String shipping_company;

	@ZteSoftCommentAnnotationParam(name = "logi_no", type = "String", isNecessary = "N", desc = "logi_no：物流单号")
	private String logi_no;

	public String getShip_name() {
		return ship_name;
	}

	public void setShip_name(String ship_name) {
		this.ship_name = ship_name;
	}

	public String getShip_province() {
		return ship_province;
	}

	public void setShip_province(String ship_province) {
		this.ship_province = ship_province;
	}

	public String getShip_city() {
		return ship_city;
	}

	public void setShip_city(String ship_city) {
		this.ship_city = ship_city;
	}

	public String getShip_district() {
		return ship_district;
	}

	public void setShip_district(String ship_district) {
		this.ship_district = ship_district;
	}

	public String getShip_addr() {
		return ship_addr;
	}

	public void setShip_addr(String ship_addr) {
		this.ship_addr = ship_addr;
	}

	public String getShipping_company() {
		return shipping_company;
	}

	public void setShipping_company(String shipping_company) {
		this.shipping_company = shipping_company;
	}

	public String getLogi_no() {
		return logi_no;
	}

	public void setLogi_no(String logi_no) {
		this.logi_no = logi_no;
	}

}
